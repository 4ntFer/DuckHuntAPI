<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="DuckHuntAPI.Models" assembly="DuckHuntAPI">

    <class name="Character" table="Character">

        <id name="id">
            <column name="id" sql-type="int" not-null="false"/>
            <generator class="uuid.hex" />
        </id>

        <property name="name">
            <column name="Name" length="255" not-null="false" />
        </property>
    </class>

</hibernate-mapping>